@model PresentationLayer.Models.LevelViewModel
@{
    ViewBag.Title = "CreateNewLevel";
    }

    <!-- BOOTSTRAP STYLES-->
    <link href="~/assets/css/bootstrap.css" rel="stylesheet" />
    <!-- FONTAWESOME STYLES-->
    <link href="~/assets/css/font-awesome.css" rel="stylesheet" />
    <!-- CUSTOM STYLES-->
    <link href="~/assets/css/custom.css" rel="stylesheet" />
    <!-- GOOGLE FONTS-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />
<link href="~/assets/css/registration.css" rel="stylesheet" />
    <div id="wrapper">
     <div>
       @Html.Partial("~/Views/Shared/navigator.cshtml")                
     </div>
     <div id="page-wrapper">
            <div id="page-inner">

@using (Html.BeginForm("SaveLevel", "Level", FormMethod.Post))
{ 
        <h2>Form To Create New Level</h2>
             <div class="form-group">
                @Html.LabelFor(x=> x.Year)
                @Html.EditorFor(x=> x.Year)
            </div>
            <div class="form-group">
                @Html.LabelFor(x=> x.Semester)
                @Html.EditorFor(x=> x.Semester)
            </div>
            <div class="form-group">
                @Html.LabelFor(x=> x.Description)
                @Html.EditorFor(x=> x.Description)
            </div>
                    <button type="submit">Create New Level</button>
                      
}
                </div>
         </div>
        </div>
   
<!-- JQUERY SCRIPTS -->
    <script src="~/assets/js/jquery-1.10.2.js"></script>
    <!-- BOOTSTRAP SCRIPTS -->
    <script src="~/assets/js/bootstrap.min.js"></script>
    <!-- METISMENU SCRIPTS -->
    <script src="~/assets/js/jquery.metisMenu.js"></script>
    <!-- CUSTOM SCRIPTS -->
    <script src="~/assets/js/custom.js"></script>
