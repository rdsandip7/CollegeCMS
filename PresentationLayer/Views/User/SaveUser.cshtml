@model PresentationLayer.Models.UserViewModel
@{
    ViewBag.Title = "SaveUser";
}
    <!-- BOOTSTRAP STYLES-->
    <link href="~/assets/css/bootstrap.css" rel="stylesheet" />
    <!-- FONTAWESOME STYLES-->
    <link href="~/assets/css/font-awesome.css" rel="stylesheet" />
    <!-- CUSTOM STYLES-->
    <link href="~/assets/css/custom.css" rel="stylesheet" />
    <!-- GOOGLE FONTS-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />
    <link href="~/assets/css/registration.css" rel="stylesheet" />
    <div id="wrapper">
         <div>
           @Html.Partial("~/Views/Shared/navigator.cshtml")                
         </div>
        <div id="page-wrapper">
            <div id="page-inner">
                <h2>SaveUser</h2>
                <table>
                    <tbody>
                        <th><h2>Following User Information Has Been Saved</h2></th>
                        <tr>
                            <td>@Html.LabelFor(x=> x.FirstName) :</td>
            
                        </tr>
                        <tr>
                            <td> @Html.LabelFor(x=> x.MiddleName) :</td>
                            <td>@Model.MiddleName</td>
                        </tr>
                        <tr>
                            <td>@Html.LabelFor(x => x.LastName) :</td>
                            <td>@Model.LastName</td>
                        </tr>
                        <tr>
                            <td>@Html.LabelFor(x => x.Username) :</td>
                            <td>@Model.Username</td>
                        </tr>
                        <tr>
                            <td>@Html.LabelFor(x => x.Email) :</td>
                            <td>@Model.Email</td>
                        </tr>
                    </tbody>
                </table>
             </div>
         </div>
     </div>
<!-- JQUERY SCRIPTS -->
    <script src="~/assets/js/jquery-1.10.2.js"></script>
    <!-- BOOTSTRAP SCRIPTS -->
    <script src="~/assets/js/bootstrap.min.js"></script>
    <!-- METISMENU SCRIPTS -->
    <script src="~/assets/js/jquery.metisMenu.js"></script>
    <!-- CUSTOM SCRIPTS -->
    <script src="~/assets/js/custom.js"></script>